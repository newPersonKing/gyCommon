<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gy.com.gycommon">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".TestActivity"
            android:theme="@style/toolBarTheme"/>
        <activity android:name=".activity.TabFragmentActivity"/>
        <activity android:name=".activity.ViewPagerFragmentActivity"/>
        <activity android:name=".PreActivity"/>
        <activity android:name=".preview.tab.FlycoTabLayoutCommonPreActivity"/>
        <activity android:name=".preview.tab.FlycoTablayoutSegmentPreActivity"/>
        <activity android:name=".preview.tab.FlycoTablayoutSlidingActivity"/>

        <activity android:name=".preview.drawerLayout.SlidingRootNavActivity"
            android:theme="@style/toolBarTheme"/>
    </application>

</manifest>