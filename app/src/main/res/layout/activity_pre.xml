<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:gravity="center">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical">
        <Button
            android:id="@+id/pre_tab_common"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="pre_tab_common"/>
        <Button
            android:id="@+id/pre_tab_segment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="pre_tab_segment"/>

        <Button
            android:id="@+id/pre_tab_sliding"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="pre_tab_sliding"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical">
        <Button
            android:id="@+id/drawerlayout_fantasy_slide"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="drawerlayout_fantasy_slide"/>
    </LinearLayout>


</LinearLayout>
